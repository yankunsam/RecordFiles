/*docker/daemon/start.go*/
|->Start()
 |->containerStart()
  |->createSpec()
   |->setDevices()
   |->setRlimits()
   |->setUser()
   |->setNamespaces(daemon *Daemon, s *specs.Spec,c *container.Container))[ spec is the base configuration for the container]
    |->ns := specs.Namespace{ Type:"pid"}   [ Here, we choose the pid_namespace :)]
    |->pc,err := daemon.getPidContainer(c)
    |->ns.Path = fmt.Sprintf("/proc/%d/ns/pid",pc.State.GetPID())
    |->setNamespace(s,ns)
     |->append()
   |->setCapabilities()
  |->daemon.containerd.Create() [ containerd: libcontainerd.Client]
   |->clnt.newContainer()  

|->ContainerCreate()
 |->containerCreate()
  |->daemon.verifyContainerSettings()
  |->daemon.verifyNetworkingConfig()
  |->daemon.adaptContainerSettings()
  |->daemon.create()
   |->daemon.GetImage()
   |->daemon.mergeAndVerifyConfig()
   |->daemon.mergeAndVerifyLogConfig()
   |->daemon.newContainer()


/* package ns [vendor/github.com/docker/libnetwork/ns/init_linux.go ]*/
|->SetNamespace
 |->netns.Set()
